  "version": "1.0.0",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.37",
    "mocha": "^5.2.0",
    "svelte": "^3.44.0",
    "vite": "^2.8.0",
    "vite-plugin-monaco-editor": "^1.0.10",
    "webgme": "^2.23.0",
    "webgme-cli": "^2.8.5"
  },
  "dependencies": {
    "monaco-editor": "^0.30.0",
    "monaco-yaml": "^4.0.0-alpha.1"
  }
